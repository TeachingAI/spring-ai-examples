        <module>spring-ai-ollama-prompt</module>
        <module>spring-ai-ollama-langfuse</module>
        <module>spring-ai-ollama-chroma-store</module>
        <module>spring-ai-ollama-milvus-store</module>
        <module>spring-ai-ollama-neo4j-store</module>
        <module>spring-ai-ollama-pgvector-store</module>
        <module>spring-ai-ollama-pinecone-store</module>
        <module>spring-ai-ollama-qdrant-store</module>
        <module>spring-ai-ollama-redis-store</module>
        <module>spring-ai-ollama-weaviate-store</module>-->
        <module>spring-ai-openai</module>
        <module>spring-ai-sql</module>
        <module>spring-ai-stabilityai</module>
        <module>spring-ai-vertexai-palm2</module>
        <module>spring-ai-vertexai-gemini</module>
        <!-- 基于 智普AI 的相关整合示例 -->
        <module>spring-ai-zhipuai</module>
        <module>spring-ai-zhipuai-functions</module>
        <module>spring-ai-zhipuai-prompt</module>
        <module>spring-ai-zhipuai-langfuse</module>
        <module>spring-ai-zhipuai-chroma-store</module>
        <module>spring-ai-zhipuai-milvus-store</module>
        <module>spring-ai-zhipuai-neo4j-store</module>
        <module>spring-ai-zhipuai-pgvector-store</module>
        <module>spring-ai-zhipuai-pinecone-store</module>
        <module>spring-ai-zhipuai-qdrant-store</module>
        <module>spring-ai-zhipuai-redis-store</module>
        <module>spring-ai-zhipuai-weaviate-store</module>
        <module>spring-ai-stepfun</module>
    </modules>

	<repositories>
        <repository>
            <id>spring-milestone</id>
            <name>Spring Milestone Repository</name>
            <url>https://repo.spring.io/milestone</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
    </repositories>

    <dependencyManagement>
        <dependencies>
            <!-- https://mvnrepository.com/artifact/com.alibaba.fastjson2/fastjson2 -->
            <dependency>
                <groupId>com.alibaba.fastjson2</groupId>
                <artifactId>fastjson2</artifactId>
                <version>${fastjson2.version}</version>
            </dependency>
            <!-- For Chat Completion & Embedding -->
            <dependency>
                <groupId>org.springframework.ai</groupId>
                <artifactId>spring-ai-azure-openai-spring-boot-starter</artifactId>
                <version>${spring-ai.version}</version>
                <exclusions>
                    <exclusion>
                        <groupId>org.springframework.boot</groupId>
                        <artifactId>spring-boot-starter-logging</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>com.github.teachingai</groupId>
                <artifactId>spring-ai-baidu-ai-qianfan-spring-boot-starter</artifactId>
